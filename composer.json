    "require": {
        "illuminate/database": "^8.21",
        "illuminate/support": "^8.0|^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Job Verplanke",
            "email": "job.verplanke@gmail.com"
        }
    ],
    "config": {
